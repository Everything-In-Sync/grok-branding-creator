{
  "name": "grokbrandcreator",
  "version": "1.0.0",
  "description": "Branding Package Generator - Generate professional brand starter packages from industry input",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "start": "node dist/server/index.js",
    "test": "npm run test:server && npm run test:client",
    "test:server": "node --test server/**/*.test.js",
    "test:client": "vitest"
  },
  "keywords": [
    "branding",
    "color-palette",
    "generator"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/archiver": "^6.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "archiver": "^7.0.1",
    "canvas": "^3.2.0",
    "chroma-js": "^3.1.2",
    "color-contrast-calc": "^0.4.1",
    "cors": "^2.8.5",
    "d3-color": "^3.1.0",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tinycolor2": "^1.6.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "devDependencies": {
    "@types/chroma-js": "^3.1.1",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
